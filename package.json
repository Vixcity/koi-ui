    "element-plus": "^2.4.2",
    "js-cookie": "^3.0.5",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "sortablejs": "^1.15.0",
    "vue": "^3.3.7",
    "vue-router": "^4.2.5"
  },
